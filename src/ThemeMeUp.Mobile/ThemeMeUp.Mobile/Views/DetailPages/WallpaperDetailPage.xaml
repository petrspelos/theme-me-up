<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="ThemeMeUp.Mobile.Views.DetailPages.WallpaperDetailPage"
             xmlns:viewModels="clr-namespace:ThemeMeUp.Mobile.ViewModels;assembly=ThemeMeUp.Mobile"
             x:DataType="viewModels:WallpaperDetailPageViewModel">
    <ContentPage.Content>
        <StackLayout>
            <Label Text="{Binding Wallpaper.FullImageUrl}"
                VerticalOptions="CenterAndExpand" 
                HorizontalOptions="CenterAndExpand" TextColor="Red" />
        </StackLayout>
    </ContentPage.Content>
</ContentPage>